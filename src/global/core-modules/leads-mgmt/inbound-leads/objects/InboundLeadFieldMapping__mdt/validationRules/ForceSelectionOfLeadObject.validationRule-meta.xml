<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ForceSelectionOfLeadObject</fullName>
    <active>true</active>
    <errorConditionFormula>AND(LeadObject__r.DeveloperName != &apos;Lead&apos;, LeadObject__r.DeveloperName != &apos;LeadInteractionEntry&apos;)</errorConditionFormula>
    <errorDisplayField>LeadObject__c</errorDisplayField>
