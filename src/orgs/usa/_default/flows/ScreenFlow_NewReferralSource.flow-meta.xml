            </connector>
            <label>Practitioner</label>
        </rules>
        <rules>
            <name>Decision_WhatTypeOfReferralWasChosen_Other</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>RadioButton_TypeOfReferral</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>Choice_TypeOfReferral_OtherEntities</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Screen_OtherReferralSourceSelection</targetReference>
            </connector>
            <label>Other</label>
        </rules>
    </decisions>
    <description>ScreenFlow to allow the creation of new Referral Sources in a Lead or Patient.</description>
    <dynamicChoiceSets>
        <name>RecordChoiceSet_Practitioners</name>
        <dataType>String</dataType>
        <displayField>ContactName__c</displayField>
        <filterLogic>and</filterLogic>
        <filters>
            <field>EmrServer__c</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>emrServerId</elementReference>
            </value>
        </filters>
        <filters>
            <field>ArtemisPractitionerIdentifier__c</field>
            <operator>IsNull</operator>
            <value>
                <booleanValue>false</booleanValue>
            </value>
        </filters>
        <object>EmrServerContactReference__c</object>
        <valueField>Contact__c</valueField>
    </dynamicChoiceSets>
