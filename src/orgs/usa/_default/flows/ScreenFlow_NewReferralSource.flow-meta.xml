    <decisions>
        <name>Decision_LeadDoesNotHaveEmrServer</name>
        <label>Lead does not have a EMR Server?</label>
        <locationX>182</locationX>
        <locationY>458</locationY>
        <defaultConnector>
            <targetReference>Screem_TypeOfReferral</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>Decision_LeadDoesNotHaveEmrServer_Yes</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>emrServerId</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Screen_NoEmrServerOnLead</targetReference>
            </connector>
            <label>Yes (it doesn&apos;t)</label>
        </rules>
    </decisions>
    <decisions>
        <name>Decision_PatientDoesNotHavePrimaryEmrServer</name>
        <label>Patient does not have a Primary EMR Server?</label>
        <locationX>622</locationX>
        <locationY>458</locationY>
        <defaultConnector>
            <targetReference>Screem_TypeOfReferral</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>Decision_PatientDoesNotHavePrimaryEmrServer_Yes</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>emrServerId</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Screen_NoPrimaryEmrServerOnPatient</targetReference>
            </connector>
            <label>Yes (it doesn&apos;t)</label>
        </rules>
    </decisions>
    <decisions>
        <name>Decision_WhatTypeIsTheSourceRecord</name>
        <label>What type is the source record?</label>
        <locationX>556</locationX>
        <locationY>134</locationY>
        <defaultConnector>
            <targetReference>Screen_InvalidSourceRecordProvided</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Invalid</defaultConnectorLabel>
        <rules>
            <name>Lead</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>recordId</leftValueReference>
                <operator>StartsWith</operator>
                <rightValue>
                    <stringValue>00Q</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assignment_SaveAsLeadId</targetReference>
            </connector>
            <label>Lead</label>
        </rules>
        <rules>
            <name>Patient</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>recordId</leftValueReference>
                <operator>StartsWith</operator>
                <rightValue>
                    <stringValue>001</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assignment_SaveAsPatientId</targetReference>
            </connector>
            <label>Patient (Account)</label>
        </rules>
    </decisions>
    <decisions>
        <name>Decision_WhatTypeOfReferralWasChosen</name>
        <label>What type of referral was chosen?</label>
        <locationX>556</locationX>
        <locationY>1058</locationY>
        <defaultConnectorLabel>Invalid option</defaultConnectorLabel>
        <rules>
            <name>Decision_WhatTypeOfReferralWasChosen_Practitioner</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>RadioButton_TypeOfReferral</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>Choice_TypeOfReferral_Practitioner</elementReference>
                </rightValue>
            </conditions>
            <connector>
